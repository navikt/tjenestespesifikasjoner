<jaxws:bindings xmlns:jaxws="https://jakarta.ee/xml/ns/jaxws"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
	
    <!-- forhåpentligvis midlertidig fiks for å få navnet på generert porttype-klasse til
     å matche navnet i wsdlen (pga av underscore i navnet i wsdl som blir borte ved wsdl2java -->
    <jaxws:bindings node="wsdl:definitions//wsdl:portType[@name='notat_v1']">
        <jaxws:class name="Notat_V1"/>
    </jaxws:bindings>
</jaxws:bindings>
