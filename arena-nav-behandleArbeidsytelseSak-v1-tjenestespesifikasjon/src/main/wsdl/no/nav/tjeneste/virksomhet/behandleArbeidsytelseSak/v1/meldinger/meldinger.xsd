    <xsd:complexType name="OpprettSakResponse">
        <xsd:sequence>
            <xsd:element name="saksnummer" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Formatet er et 4-sifret aarstall etterfulgt av et loepenummer p√• inntil 7 sifre. Ingen skilletegn mellom aarstall og loepenr.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
