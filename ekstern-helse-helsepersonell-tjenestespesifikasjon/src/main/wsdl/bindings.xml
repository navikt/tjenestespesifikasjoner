<jaxws:bindings version="3.0"
                xmlns="https://jakarta.ee/xml/ns/jaxws"
                xmlns:jaxws="https://jakarta.ee/xml/ns/jaxws"
                xmlns:jxb="https://jakarta.ee/xml/ns/jaxb" jxb:version="xjc"
                xmlns:xs="http://www.w3.org/2001/XMLSchema"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                wsdlLocation="helsepersonell.wsdl"
                xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" jxb:extensionBindingPrefixes="xjc"


>
    <jxb:globalBindings generateElementProperty="false">
        <xjc:javaType name="java.time.LocalDateTime" xmlType="xs:dateTime" adapter="io.github.threetenjaxb.core.LocalDateTimeXmlAdapter" />
        <xjc:simple />
    </jxb:globalBindings>
    <jaxws:bindings node="wsdl:definitions/wsdl:types/xs:schema">
        <jxb:globalBindings generateElementProperty="false">
            <xjc:javaType name="java.time.LocalDateTime" xmlType="xs:dateTime" adapter="io.github.threetenjaxb.core.LocalDateTimeXmlAdapter" />
            <xjc:simple/>
        </jxb:globalBindings>
        <jxb:schemaBindings >

            <jxb:nameXmlTransform>
                <jxb:elementName prefix="WS"/>
                <jxb:typeName prefix="WS"/>
                <jxb:anonymousTypeName prefix="WS"/>
            </jxb:nameXmlTransform>
        </jxb:schemaBindings>
    </jaxws:bindings>
    </jaxws:bindings>
